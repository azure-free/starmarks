{
  "name": "starmarks",
  "version": "0.1",
  "description": "A chrome bookmark manager with ratings",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "starmarks",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 1,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
